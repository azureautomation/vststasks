{
    "manifestVersion": 1,
    "id": "azureautomation",
    "version": "1.0.0",
    "name": "Azure Automation Tasks",
    "description": "Tasks for creating an automation account, importing and starting runbooks, importing, compiling, and assigning DSC configurations, and uploading modules from VSTS build artifacts.",
    "publisher": "",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
        ],
    "icons": {
        "default": "icon.png"
     },
    "contributions": [
        {
            "id": "AzureAutomationDsc",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
              ],
              "properties": {
                "name": "AzureAutomationDsc"
              }
        },
        {
            "id": "AzureAutomationAccount",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
              ],
              "properties": {
                "name": "AzureAutomationAccount"
              }
        },
        {
            "id": "AzureAutomationRunbook",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
              ],
              "properties": {
                "name": "AzureAutomationRunbook"
              }
        },
        {
            "id": "AzureAutomationModules",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
              ],
              "properties": {
                "name": "AzureAutomationModules"
              }
        }
    ],
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "icon.png", "addressable": true
        },
        {
            "path": "AzureAutomationAccount", "addressable": true
        },
        {
            "path": "AzureAutomationDsc", "addressable": true
        },
        {
            "path": "AzureAutomationRunbook", "addressable": true
        },
        {
            "path": "AzureAutomationModules", "addressable": true
        }
    ]
}
